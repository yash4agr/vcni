# VCNI Environment Variables
# Copy this file to .env and fill in the values

# AssemblyAI Configuration (Required for voice input)
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Modal Classifier Configuration
MODAL_CLASSIFIER_URL=https://yash4agr--joint-nlu-service-web-inference.modal.run
CLASSIFIER_TIMEOUT=10

# Intent Classification
CONFIDENCE_THRESHOLD=0.7

# Wix Configuration (Required for frontend build)
WIX_CLIENT_ID=your_wix_client_id_here
WIX_CLIENT_INSTANCE_ID=local-dev
WIX_CLIENT_PUBLIC_KEY=local-dev
WIX_CLIENT_SECRET=local-dev

# Frontend API URL (for production)
# API_URL=http://backend:8000
